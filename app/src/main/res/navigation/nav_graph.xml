<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/breedFragment">
    <fragment
        android:id="@+id/breedFragment"
        android:name="com.example.updog.ui.BreedFragment"
        android:label="Breeds"
        tools:layout="@layout/fragment_breed">
        <action
            android:id="@+id/action_breedFragment_to_imageFragment"
            app:destination="@id/imageFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_breedFragment_to_subbreedFragment"
            app:destination="@id/subbreedFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/imageFragment"
        android:name="com.example.updog.ui.ImageFragment"
        android:label="Images"
        tools:layout="@layout/fragment_image">
        <action
            android:id="@+id/action_imageFragment_to_subbreedFragment"
            app:destination="@id/subbreedFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_imageFragment_to_breedFragment"
            app:destination="@id/breedFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/subbreedFragment"
        android:name="com.example.updog.ui.SubbreedFragment"
        android:label="Subbreeds"
        tools:layout="@layout/fragment_subbreed">
        <action
            android:id="@+id/action_subbreedFragment_to_imageFragment"
            app:destination="@id/imageFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_subbreedFragment_to_breedFragment"
            app:destination="@id/breedFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
</navigation>